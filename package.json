{
    "name": "vite-wallet",
    "version": "1.3.9",
    "main": "app/main.js",
    "description": "The Desktop app for vite.",
    "author": "Vitelabs",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vitelabs/vite-wallet.git"
    },
    "scripts": {
        "clean": "rm -rf ./app",
        "build": "gulp && NODE_ENV=prod node build.js",
        "builddev": "gulp && NODE_ENV=dev node build.js",
        "build:win": "p=WIN npm run build",
        "dev": "NO_BUILD=true npm run builddev && NODE_ENV=dev HOT_RELOAD=true electron .",
        "dev:nohot": "NO_BUILD=true npm run builddev && NODE_ENV=dev electron .",
        "test": "mocha",
        "lint": "eslint ./ --cache",
        "devd": "gulp",
        "release": "RELEASE=true npm run build",
        "release:win": "RELEASE=true npm run build:win"
    },
    "devDependencies": {
        "babel-core": "^6.26.3",
        "babel-eslint": "^8.2.6",
        "babel-preset-es2016-node5": "^1.1.2",
        "dotenv": "^8.2.0",
        "electron": "9.0.3",
        "electron-builder": "^22.7.0",
        "electron-notarize": "^1.0.0",
        "eslint": "~4.19.1",
        "eslint-plugin-vue": "~4.7.0",
        "event-pubsub": "^4.3.0",
        "gulp": "^4.0.2",
        "gulp-babel": "~7.0.1",
        "js-queue": "^2.0.0",
        "mocha": "^5.2.0"
    },
    "dependencies": {
        "@trodi/electron-splashscreen": "^1.0.0",
        "auto-launch": "^5.0.5",
        "cross-spawn": "^6.0.5",
        "electron-log": "^4.2.1",
        "electron-serve": "^1.0.0",
        "electron-store": "^5.2.0",
        "electron-updater": "^4.3.1",
        "menubar": "^9.0.1",
        "semver": "^7.3.2"
    },
    "electronVersion": "9.0.3",
    "build": {
        "afterSign": "build/notarize.js"
    }
}
